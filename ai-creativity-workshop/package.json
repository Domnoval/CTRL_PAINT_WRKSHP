    "@nx/react": "^17.3.0",
    "@nx/next": "^17.3.0",
    "@nx/node": "^17.3.0",
    "@nx/eslint": "^17.3.0",
    "@nx/vite": "^17.3.0",
    "@nx/playwright": "^17.3.0",

    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nx": "^17.3.0",
    "prettier": "^2.8.0",
    "supabase": "^1.102.3",
    "tsx": "^3.14.0",
    "typescript": "^5.1.3",
